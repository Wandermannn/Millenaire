{
	"multipart": [
        {   "apply": { "model": "millenaire:painted_brick_lime" }
        },
        {   "when": { "top_frieze":"true" },
            "apply": { "model": "millenaire:painted_brick_decoration1_top" }
        },
        {   "when": { "bottom_frieze":"true", "top_frieze":"false" },
            "apply": { "model": "millenaire:painted_brick_decoration1_bottom" }
        },
        {   "when": { "bottom_frieze":"false", "top_frieze":"false" },
            "apply": { "model": "millenaire:painted_brick_decoration1" }
        }
        
    ]
}
